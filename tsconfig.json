{
  "extends": "./.nuxt/tsconfig.json",
  "include": [
    "nuxt.d.ts",
    "types",
    "composables",
    "plugins",
    "runtimeConfig.d.ts",
    "nuxt.config.ts",
    ".nuxt",
    "utils/authUtils.ts"
  ],
  "compilerOptions": {
    "types": ["vitest/globals", "nuxt"],
    "typeRoots": ["./types", "./node_modules/@types"],
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"]
    }
  }
}
